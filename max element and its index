n=int(input("enter the elements"))
a=[]
for i in range(n):
    e=int(input("enter the values to store"))
    a.append(e)
print(a)
one=[]
two=[]
for j in a:
    if a.index(j)%2==1:
        one.append(j)
    else:
        two.append(j)

q=max(one)
one.remove(q)
q=max(one)

p=max(two)
two.remove(p)
p=max(two)

print(p+q)
