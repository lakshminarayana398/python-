def checkpassword(str):
    c1=0
    c2=0
    c3=0
    c4=0
    if str[0] in "0123456789":
        return 0
    for i in str:
        if i in "0123456789":
            c2+=1
        if i.isupper():
            c3+=1
        if i ==" " or i=="/":
            c4+=1
    if c4>=1:
        return 0
    if len(str)>=4:
        if c2>=1 and c3>=1:
            return 1
        else:return 0
    else:return 0


str=input("enter the pass")
print(checkpassword(str))
